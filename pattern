import java.util.*;
 public class pattern{
    public static void main(string args[]){
        int n=4:
        int m=5;
        for(i=1;i<=n;i++){
            for(j=1;j<=m;j++){
                //cell(i,j)
                if(j=1||j=n||i=n||i=n){
                    system.out.print("*");
                    else{
                    system.out.println("");
                    }
                }
            }
            system.out.println();
        }
    }
 }